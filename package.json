{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "sugardaddydiabetes",
  "title": "SugarDaddyDiabetes",
  "description": "It helps monitory glucose data from Freestyle Libre 2 & 3 devices",
  "icon": "extension-icon.png",
  "author": "authormatic",
  "owner": "magi",
  "categories": [
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "diabetes",
      "title": "View Glucose Readings",
      "description": "Show glucose readings from LibreView",
      "mode": "view"
    },
    {
      "name": "menubar",
      "title": "Glucose Menu Bar",
      "description": "Show latest glucose reading in menu bar",
      "mode": "menu-bar",
      "interval": "5m",
      "persistent": true
    }
  ],
  "preferences": [
    {
      "name": "username",
      "type": "textfield",
      "required": true,
      "title": "LibreView Username",
      "description": "Your LibreView account email"
    },
    {
      "name": "password",
      "type": "password",
      "required": true,
      "title": "LibreView Password",
      "description": "Your LibreView account password"
    },
    {
      "name": "unit",
      "type": "dropdown",
      "required": true,
      "title": "Glucose Unit",
      "description": "Choose your preferred glucose unit",
      "data": [
        {
          "title": "mmol/L",
          "value": "mmol"
        },
        {
          "title": "mg/dL",
          "value": "mgdl"
        }
      ],
      "default": "mmol"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.86.0",
    "@raycast/utils": "^1.17.0",
    "axios": "^1.7.7",
    "date-fns": "^3.6.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  }
}
